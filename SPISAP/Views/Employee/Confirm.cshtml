        }
        else
        {
        <select class="form-control" id="chemise" name="CHEMISE">
        <option value=" ">Seleccione un valor</option>
        </select>                                    
        }
        </div>
        <div class="col-md-4">
        Talla de Pantalón :
        @if ( Model.TallaPantalon != null )
        {
        @Html.DropDownListFor(model => model.PANTALON, new SelectList(Model.TallaPantalon, "CODIGO", "DESCRIPCION", Model.TallaPantalon), new { @class = "form-control", @id = "pantalon", @disabled="disabled"})
        }
        else
        {
        <select class="form-control" id="pantalon" name="PANTALON">
