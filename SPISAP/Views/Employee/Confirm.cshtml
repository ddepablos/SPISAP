        {
        @Html.DropDownListFor(model => model.CALZADO, new SelectList(Model.TallaCalzado, "CODIGO", "DESCRIPCION", Model.TallaCalzado), new { @class = "form-control", @id = "calzado", @disabled="disabled"})                        
        }
