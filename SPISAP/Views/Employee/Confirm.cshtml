        <option value=" ">Seleccione un valor</option>
        </select>                       
        }
        </div>
        <div class="col-md-3">
        Ciudad :
        @Html.TextBoxFor(model => model.CIUDAD, new { @class = "form-control", @maxlength="40", @disabled="disabled"})
        </div>
