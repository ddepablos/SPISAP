@model SPISAP.Models.EmployeeViewModel

@{
    ViewBag.Title = "Confirm";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<nav class="navbar navbar-inverse navbar-fixed-top">
    <div class="container-fluid">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand">Gestión de Registros de Trabajadores</a>
        </div>
        <div id="navbar" class="navbar-collapse collapse">
            <a class="navbar-brand navbar-right">Usuario :  @Html.DisplayFor(model => model.COD_USER, new { @class="form-control", @disabled="disabled"})</a>
        </div>
    </div>
</nav>

@using (Html.BeginForm("Confirm", "Employee", "POST"))
{ 

    <div class="container jumbotron form-signin">
        
    <form class="form-horizontal">

    <h3>Confirmar Datos del Trabajador</h3>

    <div class="row">

        <!-- DATOS PERSONALES -->
        <div class="title green">DATOS PERSONALES</div>
    
        <div class="row">
            <br>
            <div class="col-md-3">
            Primer Apellido :
            @Html.TextBoxFor(model => model.PRIMER_APELLIDO, new { @class = "form-control", id = "primerapellido", @maxlength="40", @disabled="disabled"})
            </div>
            <div class="col-md-3">
            Segundo Apellido :
            @Html.TextBoxFor(model => model.SEGUNDO_APELLIDO, new { @class = "form-control", @maxlength="40", @disabled="disabled"})
            </div>
            <div class="col-md-3">
            Nombres :
            @Html.TextBoxFor(model => model.NOMBRE, new { @class = "form-control", @maxlength="40", @disabled="disabled"})
            </div>
            <div class="col-md-2">
            Cédula :
            @Html.TextBoxFor(model => model.CEDULA, new { @class = "form-control", @maxlength="10" , @style="text-align:center", @disabled="disabled"})
            </div>
            <div class="col-md-1">
            Sexo :
            @Html.DropDownListFor(model => model.SEXO, new SelectList(Model.Generos, "CODIGO", "DESCRIPCION", Model.Generos), new { @class = "form-control", id = "sexo", @disabled="disabled"})
            </div>
        </div>
    <div class="row">
        <div class="col-md-3">
        Fecha de Nacimiento :
        @Html.TextBoxFor(model => model.FECHA_NACIMIENTO, new { @class = "form-control", @maxlength="10" , @style="text-align:center", @disabled="disabled"})
        </div>
        <div class="col-md-3">
        Ciudad de Nacimiento :
        @Html.TextBoxFor(model => model.CIUDAD_NACIMIENTO, new { @class = "form-control", @maxlength="40", @disabled="disabled"})
        </div>
        <div class="col-md-3">
        País de Nacimiento :
        @Html.DropDownListFor(m => m.COD_PAIS, new SelectList(Model.Paises, "COD_PAIS", "DES_PAIS", Model.Paises), new { @class = "form-control", @id = "paisnac", @disabled="disabled"})
        </div>
        <div class="col-md-3">
        Nacionalidad :
        @Html.DropDownListFor(m => m.COD_NACIONALIDAD, new SelectList(Model.Nacionalidades, "COD_NACIONALIDAD", "DES_NACIONALIDAD", Model.Nacionalidades), new { @class = "form-control", @id = "nacionalidad", @disabled="disabled"})
        </div>
    </div>
    <div class="row">
        <div class="col-md-3">
        Estado de Nacimiento :
        @Html.DropDownListFor(m => m.COD_ESTADO, new SelectList(Model.Estados, "COD_ESTADO", "DES_ESTADO", Model.Estados), new { @class = "form-control", @id = "edonac", @disabled="disabled"})
        </div>
        <div class="col-md-3">
        Estado Civil :
        @Html.DropDownListFor(m => m.ESTADO_CIVIL, new SelectList(Model.EstadoCivil, "CODIGO", "DESCRIPCION", Model.EstadoCivil), new { @class = "form-control", @id = "edocivil", @disabled="disabled"})
        </div>
        <div class="col-md-3">
        RIF :
        @Html.TextBoxFor(model => model.RIF, new { @class = "form-control", @maxlength="20", @disabled="disabled"})
        </div>
        <div class="col-md-3">
        Correo Electrónico :
        @Html.TextBoxFor(model => model.COD_CLASE_CORREO, new { @class = "form-control", @maxlength="40", @disabled="disabled"})
        </div>
        </div>
        <div class="row">
        <div class="col-md-3">
